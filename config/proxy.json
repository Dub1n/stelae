{
  "mcpProxy": {
    "baseURL": "https://mcp.infotopology.xyz",
    "addr": ":9090",
    "name": "Stelae MCP Proxy",
    "version": "1.0.0",
    "type": "sse",
    "options": {
      "panicIfInvalid": true,
      "logEnabled": true,
      "enableManifest": true
    }
  },
  "manifest": {
    "name": "Stelae MCP Proxy",
    "version": "1.0.0",
    "description": "Local MCP stack",
    "publicBaseURL": "https://mcp.infotopology.xyz",
    "resources": []
  },
  "mcpServers": {
    "fs": {
      "type": "stdio",
      "command": "/home/gabri/.local/bin/rust-mcp-filesystem",
      "args": [
        "--root",
        "/home/gabri/dev/stelae"
      ]
    },
    "rg": {
      "type": "stdio",
      "command": "/home/gabri/.local/bin/mcp-grep-server",
      "args": [
        "--stdio",
        "--root",
        "/home/gabri/dev/stelae"
      ]
    },
    "sh": {
      "type": "stdio",
      "command": "/home/gabri/.local/bin/terminal_controller",
      "args": [
        "--workdir",
        "/mnt/c/Users/gabri/Documents/Infotopology/VDL_Vault",
        "--allow",
        "npm,pytest,make,python,git"
      ]
    },
    "docs": {
      "type": "stdio",
      "command": "/home/gabri/.local/bin/mcp-server-docy",
      "args": [
        "--stdio"
      ]
    },
    "mem": {
      "type": "stdio",
      "command": "/home/gabri/.local/bin/basic-memory",
      "args": [
        "--project",
        "stelae",
        "mcp",
        "--transport",
        "stdio"
      ]
    },
    "fetch": {
      "type": "http",
      "command": "/home/gabri/.local/bin/mcp-server-fetch"
    },
    "strata": {
      "type": "stdio",
      "command": "/home/gabri/.local/bin/strata"
    }
  }
}

