{
  "mcpProxy": {
    "baseURL": "https://mcp.infotopology.xyz",
    "addr": ":9090",
    "name": "stelae",
    "version": "1.0.0",
    "type": "streamable-http",
    "options": {
      "panicIfInvalid": true,
      "logEnabled": true,
      "enableManifest": true
    }
  },
  "manifest": {
    "name": "stelae",
    "version": "1.0.0",
    "description": "Local MCP stack",
    "publicBaseURL": "https://mcp.infotopology.xyz",
    "serverName": "stelae",
    "resources": [],
    "toolOverridesPath": "/home/gabri/dev/stelae/config/tool_overrides.json"
  },
  "mcpServers": {
    "fs": {
      "type": "stdio",
      "command": "/home/gabri/.local/bin/rust-mcp-filesystem",
      "args": [
        "/home/gabri/dev/stelae"
      ]
    },
    "rg": {
      "type": "stdio",
      "command": "/home/gabri/.local/bin/mcp-grep-server",
      "args": [
        "--stdio",
        "--root",
        "/home/gabri/dev/stelae"
      ]
    },
    "sh": {
      "type": "stdio",
      "command": "/home/gabri/.local/bin/terminal_controller",
      "args": [
        "--workdir",
        "/mnt/c/Users/gabri/Documents/Infotopology/VDL_Vault",
        "--allow",
        "npm,pytest,make,python,git"
      ]
    },
    "docs": {
      "type": "stdio",
      "command": "/home/gabri/.local/bin/mcp-server-docy",
      "args": [
        "--stdio"
      ]
    },
    "mem": {
      "type": "stdio",
      "command": "/home/gabri/.local/bin/basic-memory",
      "args": [
        "--project",
        "stelae",
        "mcp",
        "--transport",
        "stdio"
      ]
    },
    "fetch": {
      "type": "http",
      "command": "/home/gabri/.local/bin/mcp-server-fetch"
    },
    "scrapling": {
      "type": "stdio",
      "command": "python3",
      "args": [
        "/home/gabri/dev/stelae/scripts/scrapling_shim_mcp.py"
      ]
    },
    "strata": {
      "type": "stdio",
      "command": "/home/gabri/.local/bin/strata"
    },
    "one_mcp": {
      "type": "stdio",
      "command": "/home/gabri/.local/bin/uv",
      "args": [
        "--directory",
        "/home/gabri/apps/vendor/1mcpserver",
        "run",
        "server.py",
        "--local"
      ]
    }
  }
}

