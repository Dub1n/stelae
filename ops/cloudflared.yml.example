# Sample cloudflared config.
# Use ops/cloudflared.template.yml + scripts/render_cloudflared_config.py
# for real deployments. This file intentionally uses placeholders only.

tunnel: <your-tunnel-uuid-or-name>
credentials-file: /path/to/your/tunnel.json

protocol: quic
ha-connections: 4

originRequest:
  disableChunkedEncoding: true
  http2Origin: true
  keepAliveTimeout: 65s
  tcpKeepAlive: 60s

ingress:
  - hostname: mcp.your-domain.example
    service: http://127.0.0.1:9090
  - service: http_status:404
